{
  "name": "pear-md",
  "version": "1.0.0",
  "description": "markdown rendering utility for pear",
  "license": "Apache-2.0",
  "author": "Holepunch",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test:node && npm run test:bare",
    "test:node": "node test/index.node.js",
    "test:bare": "bare test/index.bare.js"
  },
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./plugins/*": "./plugins/*",
    "./package": "./package.json"
  },
  "imports": {
    "node:path": {
      "bare": "bare-path",
      "default": "path"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "path/*": {
      "bare": "bare-path/*",
      "default": "path/*"
    },
    "node:process": {
      "bare": "bare-process",
      "default": "process"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "process/*": {
      "bare": "bare-process/*",
      "default": "process/*"
    },
    "node:url": {
      "bare": "bare-url",
      "default": "url"
    },
    "url": {
      "bare": "bare-url",
      "default": "url"
    },
    "url/*": {
      "bare": "bare-url/*",
      "default": "url/*"
    }
  },
  "devDependencies": {
    "brittle": "^3.19.1"
  },
  "dependencies": {
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.2",
    "bare-url": "^2.3.2",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-select": "^6.0.4",
    "hastscript": "^9.0.1",
    "raw": "^0.1.4",
    "rehype": "^13.0.2",
    "rehype-format": "^5.0.1",
    "rehype-pretty-code": "^0.14.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sanitize": "^2.1.2",
    "unified": "^11.0.5"
  }
}
